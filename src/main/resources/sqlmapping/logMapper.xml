<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="log" >
	<!-- 插入日志信息 -->
	<insert id="insertLogSql" parameterType="java.util.HashMap">
			INSERT INTO system_log(id,classname,superclass,isinterface,target,proxy,method,nameandargs,createdate) values (
				#{id},#{classname},#{superclass},#{isinterface},#{target},#{proxy},#{method},#{nameandargs},#{createdate}
			)
	</insert>
</mapper>